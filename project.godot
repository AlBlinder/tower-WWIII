;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Tower- Wwiii"
run/main_scene="res://Canviador_escenes.tscn"

[display]

window/size/width=1900
window/size/height=1000
window/size/borderless=true
window/size/fullscreen=true
mouse_cursor/custom_image="res://UI/Altres/PNG/cursor_pointerFlat_shadow.png"

[global]

cursor=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[mono]

project/assembly_name="Tower- Wwiii"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
